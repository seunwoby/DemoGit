

CREATE COMPUTE MODULE getZipCodes_save_Variable
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		-- CALL CopyEntireMessage();
		
		SET Environment.Variables.zipCode1 = InputLocalEnvironment.REST.Input.Parameters.zipCode1;
		SET Environment.Variables.zipCode2 = InputLocalEnvironment.REST.Input.Parameters.zipCode2;
		SET OutputLocalEnvironment.Destination.HTTP.RequestURL = 'https://viacep.com.br/ws/' || Environment.Variables.zipCode1|| '/json/';
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
